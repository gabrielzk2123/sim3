            'event'         : 'logState'
        }
        dataLayer.push(logState);

        /** 
        * DATALAYER: Homepage */
        
        if(document.location.pathname == "/"){
            dataLayer.push({
                'pageType' : 'Homepage',
                'event'    : 'homepage',
                logState
            });
        }
      
        /** 
        * DATALAYER: 404 Pages
        * Fire on 404 Pages */
      	

